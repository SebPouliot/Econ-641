@Article{Bellemare2015,
  Title                    = {Why do members of congress support agricultural protection?},
  Author                   = {Marc F. Bellemare and Nicholas Carnes},
  Journal                  = {Food Policy },
  Year                     = {2015},
  Pages                    = {20--34},
  Volume                   = {50},

  Abstract                 = {Abstract It seems paradoxical that until recently, developed countries have continued subsidizing agriculture even though their agricultural sectors had been declining in relative importance since the middle of the 20\textsuperscript{th} century. What drives support for agricultural protection{\^{a}}{\texteuro}{\textquotedblright}the broad array of subsidies to farmers and taxes and quotas imposed on agricultural imports{\^{a}}{\texteuro}{\textquotedblright}in developed countries? We answer this question by testing three competing hypotheses about what drives support for agricultural protection in the US: (i) legislator preferences, (ii) electoral incentives, or (iii) lobbying. Using data on the roll call votes of the members of the 106\textsuperscript{th} through the 110\textsuperscript{th} Congresses (1999{\^{a}}{\texteuro}{\textquotedblleft}2009) and the scores given to each legislator by the Farm Bureau, our findings suggest that electoral incentives explain a great deal of the variation in support for agricultural protection, but that legislator preferences and lobbying might play a role, too. Moreover, legislator preferences and electoral incentives appear to be substitutes for one another. Why does Congress support agricultural protection? Because many members have electoral incentives to{\^{a}}{\texteuro}{\textquotedblright}and because many of those who do not still have other personal or strategic interests at stake. },
  Doi                      = {10.1016/j.foodpol.2014.10.010},
  ISSN                     = {0306-9192},
  Keywords                 = {Agricultural policy},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0306919214001523}
}

