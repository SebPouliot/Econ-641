@Article{Fargione2008,
  Title                    = {Land Clearing and the Biofuel Carbon Debt},
  Author                   = {Fargione, Joseph and Hill, Jason and Tilman, David and Polasky, Stephen and Hawthorne, Peter},
  Journal                  = {Science},
  Year                     = {2008},
  Number                   = {5867},
  Pages                    = {1235--1238},
  Volume                   = {319},

  Abstract                 = {Increasing energy use, climate change, and carbon dioxide (CO2) emissions from fossil fuels make switching to low-carbon fuels a high priority. Biofuels are a potential low-carbon energy source, but whether biofuels offer carbon savings depends on how they are produced. Converting rainforests, peatlands, savannas, or grasslands to produce food crop--based biofuels in Brazil, Southeast Asia, and the United States creates a {\textquotedblleft}biofuel carbon debt{\textquotedblright} by releasing 17 to 420 times more CO2 than the annual greenhouse gas (GHG) reductions that these biofuels would provide by displacing fossil fuels. In contrast, biofuels made from waste biomass or from biomass grown on degraded and abandoned agricultural lands planted with perennials incur little or no carbon debt and can offer immediate and sustained GHG advantages.},
  Doi                      = {10.1126/science.1152747},
  Eprint                   = {http://www.sciencemag.org/content/319/5867/1235.full.pdf},
  File                     = {:R\:\\Papers\\Fargione et al. (2008).pdf:PDF},
  Url                      = {http://www.sciencemag.org/content/319/5867/1235.abstract}
}

