@InCollection{Moschini2001,
  Title                    = {Chapter 2 Uncertainty, risk aversion, and risk management for agricultural producers},
  Author                   = {Giancarlo Moschini and David A. Hennessy},
  Booktitle                = {Agricultural Production},
  Publisher                = {Elsevier},
  Year                     = {2001},
