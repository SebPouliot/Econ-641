@Article{Karpoff2005,
  Title                    = {The Reputational Penalties for Environmental Violations: Empirical Evidence},
  Author                   = {Karpoff, Jonathan M and R,, Lott, John and Wehrly, Eric W},
  Journal                  = {Journal of Law \& Economics},
  Year                     = {2005},
  Number                   = {2},
  Pages                    = {653--75},
  Volume                   = {48},

  Abstract                 = {This paper examines the sizes of the fines, damage awards, remediation costs, and market value losses imposed on companies that violate environmental regulations. Firms that violate environmental laws suffer statistically significant losses in the market value of firm equity. The losses, however, are of similar magnitudes to the legal penalties imposed, and in the cross section, the market value loss is related to the size of the legal penalty. Thus, environmental violations are disciplined largely through legal and regulatory penalties, not through reputational penalties.},
  File                     = {:R\:\\Papers\\Karpoff, Lott & Wehrly (2005).pdf:PDF},
  Url                      = {http://econpapers.repec.org/RePEc:ucp:jlawec:y:2005:v:48:i:2:p:653-75}
}

@Article{Keane2010,
  Title                    = {A Structural Perspective on the Experimentalist School},
  Author                   = {Michael P. Keane},
  Journal                  = {Journal of Economic Perspectives},
  Year                     = {2010},

  Month                    = {Spring},
  Number                   = {2},
  Pages                    = {47--58},
  Volume                   = {24},

  Abstract                 = { What has always bothered me about the \&quot;experimentalist\&quot; school is the false sense of certainty it conveys. My view, like Leamer's, is that there is no way to escape the role of assumptions in statistical work, so our conclusions will always be contingent. Hence, we should be circumspect about our degree of knowledge. I present some lessons for economics from the field of marketing, a field where broad consensus has been reached on many key issues over the past twenty years. In marketing, 1) the structural paradigm is dominant, 2) the data are a lot better than in some fields of economics, and 3) there is great emphasis on external validation. Of course, good data always helps. I emphasize that the ability to do controlled experiments does not obviate the need for theory, and finally I address different approaches to model validation.},
  Url                      = {http://ideas.repec.org/a/aea/jecper/v24y2010i2p47-58.html}
}

