@Article{Wolff201471,
  Title                    = {Value of time: Speeding behavior and gasoline prices},
  Author                   = {Hendrik Wolff},
  Journal                  = {Journal of Environmental Economics and Management },
  Year                     = {2014},
  Number                   = {1},
  Pages                    = {71--88},
  Volume                   = {67},

  Abstract                 = {Abstract {\^{a}}{\texteuro}{\textasciitilde}Value of Time{\^{a}}{\texteuro}{\texttrademark} (VOT) is a key parameter in economics and policy. This paper presents an alternative method to estimate \{VOT\} by analyzing an hourly dataset on drivers speeding behavior as a function of the gasoline price. Our identification strategy is novel as it is based on the intensive margin. In comparison, previous studies reveal \{VOT\} on the extensive margin, but choice alternatives have multiple attributes thereby potentially confounding estimates. Consistent with the range of the prior literature, we find a \{VOT\} of about 50% of the wage rate and analyze sources of bias from accidents and traffic tickets. These bias functions suggest that previous stated preference \{VOT\} estimates are likely downward whereas previous revealed preference estimates are likely upward biased. },
  Doi                      = {10.1016/j.jeem.2013.11.002},
